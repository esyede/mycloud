
CREATE TABLE IF NOT EXISTS `akun` (
  `id` int(11) NOT NULL,
  `nama` varchar(50) NOT NULL,
  `telp` varchar(15) NOT NULL,
  `email` varchar(255) NOT NULL,
  `password` varchar(20) NOT NULL,
  `tipe` enum('dosen','mahasiswa') NOT NULL DEFAULT 'mahasiswa'
) ENGINE=InnoDB AUTO_INCREMENT=15 DEFAULT CHARSET=latin1;


INSERT INTO `akun` (`id`, `nama`, `telp`, `email`, `password`, `tipe`) VALUES
(1, 'Dosen Fasilkom', '081234567890', 'dosen@staff.unsika.ac.id', '12345', 'dosen'),
(14, 'Ardiyanto', '098765432111', '1441177004002@student.unsika.ac.id', '12345', 'mahasiswa');


CREATE TABLE IF NOT EXISTS `berita` (
  `id` int(11) NOT NULL,
  `isi` text
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1;



INSERT INTO `berita` (`id`, `isi`) VALUES
(1, 'test berita');



CREATE TABLE IF NOT EXISTS `berkas` (
  `id` int(11) NOT NULL,
  `uid` int(11) NOT NULL,
  `nama` varchar(50) NOT NULL,
  `url` varchar(100) NOT NULL,
  `tipe` varchar(30) NOT NULL,
  `ukuran` int(11) NOT NULL,
  `konten` longblob NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=35 DEFAULT CHARSET=latin1;




INSERT INTO `berkas` (`id`, `uid`, `nama`, `url`, `tipe`, `ukuran`, `konten`) VALUES
